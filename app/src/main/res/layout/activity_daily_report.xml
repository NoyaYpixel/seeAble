<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFCEF0FF"
    tools:context=".screens.DailyReport">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="329dp"
        android:layout_height="180dp"
        android:fontFamily="@font/horevclmheavywebfont"
        android:gravity="center"
        android:text="דו''ח יומי לילד"
        android:textColor="#878686"
        android:textSize="90sp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="(תאריך)"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="143dp" />

    <EditText
        android:id="@+id/etDate"
        android:layout_width="118dp"
        android:layout_height="31dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="135dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="האם הילד הגיע ?"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="196dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="האם הילד ישן ?"
        tools:layout_editor_absoluteX="286dp"
        tools:layout_editor_absoluteY="495dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="האם הילד אכל בוקר ?"
        tools:layout_editor_absoluteX="252dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="האם הילד שתה ?"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="423dp" />

    <RadioGroup
        android:id="@+id/radioGroup5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="514dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="26dp">

            <RadioButton
                android:id="@+id/rbtnYes5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="כן" />
            />

            <RadioButton
                android:id="@+id/rbtnNo5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="לא" />
            />
        </LinearLayout>
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="366dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="26dp">

            <RadioButton
                android:id="@+id/rbtnYes3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="כן" />

            <RadioButton
                android:id="@+id/rbtnNo3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="לא" />
        </LinearLayout>
    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="290dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="26dp">

            <RadioButton
                android:id="@+id/rbtnYes2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="כן" />

            <RadioButton
                android:id="@+id/rbtnNo2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="לא" />
        </LinearLayout>
    </RadioGroup>

    <EditText
        android:id="@+id/etContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/assistantbold"
        android:hint=""
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="556dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="הערות פתוחות"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="546dp" />

    <RadioGroup
        android:id="@+id/radioGroup4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="442dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="26dp">

            <RadioButton
                android:id="@+id/rbtnYes4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="כן" />

            <RadioButton
                android:id="@+id/rbtnNo4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="לא" />
        </LinearLayout>
    </RadioGroup>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/assistantbold"
        android:text="האם הילד חש בטוב?"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="272dp" />

    <RadioGroup
        android:id="@+id/radioGroup3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="215dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layoutDirection="rtl"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="26dp">

            <RadioButton
                android:id="@+id/rbtnYes1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="כן" />

            <RadioButton
                android:id="@+id/rbtnNo1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/assistantsemibold"
                android:text="לא" />
        </LinearLayout>
    </RadioGroup>

    <Button
        android:id="@+id/btnUpdating"
        android:layout_width="237dp"
        android:layout_height="37dp"
        android:background="#B1AEAE"
        android:fontFamily="@font/asakimboldwebfont"
        android:text=" עדכן דו''ח יומי "
        android:textColor="#5E5C5C"
        android:textSize="35dp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="637dp" />


</androidx.constraintlayout.widget.ConstraintLayout>